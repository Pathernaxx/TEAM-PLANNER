<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>Create Team Planner Account</title>
</head>
<body>
	<section>
		<div class="section-wrapper">
			<div class="wrapper-signup">
				<h1>Create a Team Planner Account</h1>
				<label for="username">UserName</label>
				<input type="text" name="username" id="username" placeholder="e.g., GuysSon">
				<label for="email">Email</label>
				<input type="email" name="email" id="email" placeholder="e.g., test@example.com">
				<label for="password">Password</label>
				<input type="password" name="password" id="password" placeholder="e.g.,  ••••••••••••">
				<input id="signup" type="button" value="Create New Account">
			</div>
		</div>
	</section>
</body>
</html>